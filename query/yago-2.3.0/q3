SELECT ?a1 ?a2
WHERE { 
    ?a1 <hasPreferredName> ?n1 . 
    ?a2 <hasPreferredName> ?n2 .
    ?a1 <wasBornIn> ?city1 . 
    ?a2 <wasBornIn> ?city2 . 
    ?a1 <diedIn> ?city1 . 
    ?a2 <diedIn> ?city2 . 
    ?city1 <isLocatedIn> ?c .
    ?city2 <isLocatedIn> ?c .
    ?a1 <hasWonPrize> ?w .
    ?a2 <hasWonPrize> ?w .
}